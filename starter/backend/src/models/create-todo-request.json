{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "todo",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1,
            "pattern": "^\\S"
        },
        "dueDate": {
            "type": "string"
        },
        "attachmentUrl": {
            "type": "string"
        }
    },
    "required": [
        "name",
        "dueDate"
    ],
    "additionalProperties": false
}